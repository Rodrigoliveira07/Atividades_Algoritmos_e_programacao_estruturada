#include <stdio.h>

int main() {
    int n1, n2;

    printf("Digite o primeiro número: ");
    scanf("%d", &n1);

    printf("Digite o segundo número: ");
    scanf("%d", &n2);

    if (n1 > n2) {
        printf("%d é maior do que %d", n1, n2);
    } else if (n2 > n1) {
        printf("%d é maior do que %d", n2, n1);
    } else {
        printf("%d e %d são iguais", n1, n2);
    }

    return 0;
}